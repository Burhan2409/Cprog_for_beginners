#include <stdio.h>
#include <stdlib.h>
int main()
{
    //program to add two nos
    
    int a , b , sum ;
    printf("Enter the value of a\t");
    scanf("%d",&a);
    printf("enter the value of b\t");
    scanf("%d",&b);
    sum=a+b;
    printf("The sum of values %d and %d is %d\n",a,b,sum);
    return 0;
}
